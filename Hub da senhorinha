if game.PlaceId == 3101667897 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

    local Window = Library.CreateLib("Inc0mu", "DarkTheme")

    -- Aba Player
    local TabPlayer = Window:NewTab("Player")
    local SectionPlayer = TabPlayer:NewSection("Player")

    -- Botão para aumentar a velocidade
    SectionPlayer:NewButton("High Speed (500)", "Aumenta sua velocidade (500)", function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 500
    end)

    -- Botões para aumentar o poder do Pulo
    SectionPlayer:NewButton("High JumpPower (150)", "Aumenta o poder do seu Pulo (150)", function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 150
    end)

    SectionPlayer:NewButton("High JumpPower (300)", "Aumenta o poder do seu Pulo (300)", function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 300
    end)

    SectionPlayer:NewButton("High JumpPower (500)", "Aumenta o poder do seu Pulo (500)", function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 500
    end)

    -- Botão para Vida Infinita
    SectionPlayer:NewButton("Vida Infinita", "Vida Infinita", function()
        game.Players.LocalPlayer.Character.Humanoid.Health = math.huge  -- Atribui vida infinita
    end)

    -- Botão para ficar invisível
    SectionPlayer:NewButton("Ficar Invisível", "Ficar Invisível para outros jogadores", function()
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local humanoidRootPart = character.HumanoidRootPart
            humanoidRootPart.Transparency = 1  -- Torna o HumanoidRootPart invisível
            humanoidRootPart.CanCollide = false  -- Desabilita colisão com o mundo
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Transparency = 1  -- Torna todas as partes do personagem invisíveis
                    part.CanCollide = false  -- Desabilita colisão de todas as partes
                end
            end
            -- Faz o Humanoid ficar invisível (sem colidir)
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
            end
        end
        print("Você está agora invisível!")
    end)

    -- Aba Main
    local TabMain = Window:NewTab("Main")
    local SectionMain = TabMain:NewSection("Main")

    -- Botão para coletar orbs automaticamente
    SectionMain:NewButton("Auto Collect Orbs", "Coletar Orbs Automaticamente", function()
        while true do
            local args = {
                [1] = "collectOrb";
                [2] = "Red Orb";  -- Modifique o tipo de orb aqui se necessário
                [3] = "City";     -- Modifique a cidade ou local aqui se necessário
            }
            -- Envia o remote para coletar o orb
            game:GetService("ReplicatedStorage"):WaitForChild("rEvents", 9e9):WaitForChild("orbEvent", 9e9):FireServer(unpack(args))
            wait()  -- Intervalo entre as coletas, ajuste o tempo se necessário
        end
    end)

    -- Botão para coletar gemas automaticamente
    SectionMain:NewButton("Auto Collect Gems", "Coletar Gemas Automaticamente", function()
        while true do
            local args = {
                [1] = "collectOrb";
                [2] = "Gem";      -- Tipo de orb modificado para "Gem"
                [3] = "City";     -- Cidade ou local modificado, pode ser alterado se necessário
            }
            -- Envia o remote para coletar a gema
            game:GetService("ReplicatedStorage"):WaitForChild("rEvents", 9e9):WaitForChild("orbEvent", 9e9):FireServer(unpack(args))
            wait()  -- Intervalo entre as coletas, ajuste o tempo se necessário
        end
    end)

    -- Botão para coletar bola amarela automaticamente
    SectionMain:NewButton("Auto Collect Yellow Orb", "Coletar Bola Amarela Automaticamente", function()
        while true do
            local args = {
                [1] = "collectOrb";
                [2] = "Yellow Orb";  -- Tipo de orb modificado para "Yellow Orb"
                [3] = "City";        -- Cidade ou local modificado, pode ser alterado se necessário
            }
            -- Envia o remote para coletar a bola amarela
            game:GetService("ReplicatedStorage"):WaitForChild("rEvents", 9e9):WaitForChild("orbEvent", 9e9):FireServer(unpack(args))
            wait()  -- Intervalo entre as coletas, ajuste o tempo se necessário
        end
    end)

    -- Botão para abrir o Cristal Vermelho automaticamente
    SectionMain:NewButton("Open Red Crystal", "Abrir Cristal Vermelho", function()
        local args = {
            [1] = "openCrystal";
            [2] = "Red Crystal";  -- Tipo de cristal
        }
        -- Envia o remote para abrir o Cristal Vermelho
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents", 9e9):WaitForChild("openCrystalRemote", 9e9):InvokeServer(unpack(args))
        print("Cristal Vermelho aberto")
    end)

    -- Botão para abrir o Cristal Roxo automaticamente
    SectionMain:NewButton("Open Purple Crystal", "Abrir Cristal Roxo", function()
        local args = {
            [1] = "openCrystal";
            [2] = "Purple Crystal";  -- Tipo de cristal modificado para "Purple Crystal"
        }
        -- Envia o remote para abrir o Cristal Roxo
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents", 9e9):WaitForChild("openCrystalRemote", 9e9):InvokeServer(unpack(args))
        print("Cristal Roxo aberto")
    end)
end
